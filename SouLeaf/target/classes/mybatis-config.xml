<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<!-- 만약에  null로 데이터가 전달되었으면 빈칸이 아닌 NULL 로 인식되도록 설정 -->
	</settings>
	<typeAliases>
		<typeAlias type="com.souleaf.spring.member.domain.Member" alias="Member"/>
		<typeAlias type="com.souleaf.spring.diary.domain.Diary" alias="Diary"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/diary-mapper.xml"/>	</mappers>
</configuration>