<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<!-- 만약에  null로 데이터가 전달되었으면 빈칸이 아닌 NULL 로 인식되도록 설정 -->
	</settings>
	<typeAliases>
		<typeAlias type="com.souleaf.spring.member.domain.Member" alias="Member"/>
		<typeAlias type="com.souleaf.spring.diary.domain.Diary" alias="Diary"/>
		<typeAlias type="com.souleaf.spring.diary.domain.Guestbook" alias="Guestbook"/>
		<typeAlias type="com.souleaf.spring.diary.domain.WaterDay" alias="WaterDay"/>
		<typeAlias type="com.souleaf.spring.qna.domain.Qna" alias="Qna"/>
		<typeAlias type="com.souleaf.spring.plant.domain.Plant" alias="Plant"/>
		<typeAlias type="com.souleaf.spring.plant.domain.PlantInfo" alias="PlantInfo"/>
		<typeAlias type="com.souleaf.spring.plant.domain.PlantFile" alias="PlantFile"/>
		<typeAlias type="com.souleaf.spring.companion.domain.Companion" alias="Companion"/>
		<typeAlias type="com.souleaf.spring.clinic.domain.clinic" alias="PlantFile"/>
		<typeAlias type="com.souleaf.spring.clinic.domain.ClinicCommnet" alias="ClinicCommnet"/>
		<typeAlias type="com.souleaf.spring.clinic.domain.ClinicFile" alias="ClinicFile"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/diary-mapper.xml"/>	
		<mapper resource="mappers/qna-mapper.xml"/>
		<mapper resource="mappers/plant-mapper.xml"/>
		<mapper resource="mappers/companion-mapper.xml"/>
		<mapper resource="mappers/clinic-mapper.xml"/>
		</mappers>
</configuration>